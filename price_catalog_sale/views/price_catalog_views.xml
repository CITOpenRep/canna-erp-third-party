<?xml version="1.0" ?>
<odoo>

  <record id="price_catalog_action" model="ir.actions.act_window">
    <field name="name">Price Catalogs</field>
    <field name="res_model">price.catalog</field>
    <field name="view_mode">tree,form</field>
    <field name="domain">[('catalog_type', '=', 'sale')]</field>
    <field name="context">{'default_catalog_type': 'sale'}</field>
  </record>

  <record id="price_subcatalog_action" model="ir.actions.act_window">
    <field name="name">Price Subcatalog</field>
    <field name="res_model">price.subcatalog</field>
    <field name="view_mode">tree,form</field>
    <field name="domain">[('catalog_type', '=', 'sale')]</field>
    <field name="context">{'default_catalog_type_filter': 'sale'}</field>
  </record>

</odoo>
