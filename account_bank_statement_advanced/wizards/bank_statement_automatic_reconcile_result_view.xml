<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="bank_statement_automatic_reconcile_result_view_form" model="ir.ui.view">
    <field name="name">bank.statement.automatic.reconcile.result.view.form</field>
    <field name="model">bank.statement.automatic.reconcile.result.view</field>
    <field name="arch" type="xml">
      <form string="Automatic Reconcile Result View">
        <separator colspan="4" string="Results :" />
        <field name="note" colspan="4" nolabel="1" width="850" height="400"/>
        <footer>
          <button string="Ok" class="oe_highlight" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>

</odoo>
